{
  "name": "@e22m4u/js-repository",
  "version": "0.1.16",
  "description": "Модуль для работы с базами данных для Node.js",
  "type": "module",
  "module": "./src/index.js",
  "main": "./dist/cjs/index.cjs",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/cjs/index.cjs"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "lint": "tsc && eslint .",
    "lint:fix": "tsc && eslint . --fix",
    "format": "prettier --write \"./src/**/*.js\"",
    "test": "npm run lint && c8 --reporter=text-summary mocha",
    "test:coverage": "npm run lint && c8 --reporter=text mocha",
    "build:cjs": "node build-cjs.js",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/e22m4u/js-repository.git"
  },
  "keywords": [
    "Repository",
    "ORM",
    "ODM",
    "Database",
    "Datasource",
    "Relations"
  ],
  "author": "e22m4u <e22m4u@yandex.ru>",
  "license": "MIT",
  "homepage": "https://github.com/e22m4u/js-repository",
  "peerDependencies": {
    "@e22m4u/js-format": "0.1.x",
    "@e22m4u/js-service": "0.1.x"
  },
  "devDependencies": {
    "@commitlint/cli": "~19.5.0",
    "@commitlint/config-conventional": "~19.5.0",
    "@types/chai": "~5.0.0",
    "@types/chai-as-promised": "~8.0.1",
    "@types/chai-spies": "~1.0.6",
    "@types/mocha": "~10.0.9",
    "c8": "~10.1.2",
    "chai": "~5.1.1",
    "chai-as-promised": "~8.0.0",
    "chai-spies": "~1.1.0",
    "chai-subset": "~1.6.0",
    "esbuild": "~0.24.0",
    "eslint": "~9.12.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-chai-expect": "~3.1.0",
    "eslint-plugin-jsdoc": "~50.3.1",
    "eslint-plugin-mocha": "~10.5.0",
    "husky": "~9.1.6",
    "mocha": "~10.7.3",
    "prettier": "~3.3.3",
    "tsx": "~4.19.1",
    "typescript": "~5.5.4",
    "typescript-eslint": "~8.8.1"
  }
}
